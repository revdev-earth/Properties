model Property {
    id String @id @default(uuid())

    information Information?
    legal       Legal?
    tenant      Tenant?
    insurances  Insurance[]
    services    Service[]

    economy       Economy?
    architectures Architecture[]
    equipments    Equipment[]

    notifications Notification[]
    Setting       Setting[]

    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
